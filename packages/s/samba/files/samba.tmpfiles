d /var/lib/samba/private 0700 root root -
d /var/lib/samba/bind-dns 0770 root root -

# Only members of the sambashares group can add Samba usershares and they
# can't clobber each other's shares (due to sticky bit being set).
d /var/lib/samba/usershare 1770 root sambashares -

d /var/log/samba 0755 - - -

d /run/lock/msg.lock 0755 root root -
d /run/samba 0755 root root -

d /var/cache/samba 0755 root root -
